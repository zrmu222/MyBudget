  pod 'Firebase/Database'
  pod 'Firebase/Auth'

  # Pods for MyBudget

  target 'MyBudgetTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MyBudgetUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
